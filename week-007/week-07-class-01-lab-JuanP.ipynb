   "source": [
    "def open_write_file_close(LastUser = \"JuanP\"):\n",
    "    fl = open(\"sample.txt\", \"a+\")\n",
    "    fl.write(f\"{LastUser}\")\n",
    "    fl.write(\"\\n\")\n",
    "    fl.close()\n",
    "    read_fl = open(\"sample.txt\", \"r\")\n",
    "    \n",
    "    for val in read_fl.readlines():\n",
    "      output = print(val)\n",
    "    \n",
    "    read_fl.close()\n",
    "    \n",
    "    return output \n",
    "\n",
    "open_write_file_close(\"Malcolm\")\n"
